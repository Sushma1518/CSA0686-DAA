#include<stdio.h>
int main()
{
	int n1,n2,i,gcd;
	printf("enter two integers:");
	scanf("%d%d",&n1,&n2);
	for(i=1;i<=n1 && i<=n2;i++)
	{
		if(n1%i==0 && n2%i==0)
		gcd=i;
	}
	printf("gcd of %d and %d is %d",n1,n2,gcd);
	return 0;
}

OUTPUT
a)enter two integers:36
48
gcd of 36 and 48 is 12
b)enter two integers:156
90
gcd of 156 and 90 is 6
